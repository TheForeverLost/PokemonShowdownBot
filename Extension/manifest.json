{
  "name": "PokeAssistant",
  "version": "1.0",
  "description": "Teambuilder assistant",
  "permissions": ["declarativeContent","storage","activeTab"],
  "background": {
    "scripts": ["index.js","jquery.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "32": "icons/assistant-0.png"      
    }
  },
  "icons": {
    "32": "icons/assistant-0.png"
  },
  "manifest_version": 2
}